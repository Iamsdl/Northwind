@model IEnumerable<NorthwindWeb.Models.Shippers>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}

<h2>Expeditori</h2>
<hr />
<ol class="breadcrumb">
    <li>
        @Html.ActionLink("Dashboard", "Home", "Dashboard")
    </li>
   
    <li class="active">Expeditori</li>
</ol>
<hr />
<br />
<div>
    <button class="btn btn-default crudFormButtonLeftOfSearch">@Html.ActionLink("Adauga Expeditor", "Create", null, htmlAttributes: new { style = "text-decoration: none;color:black;" })</button>
    <form action="/Shippers/Index">
        <div class="input-group cssencapsulate paddingLeftTen">
            <input type="text" name="search" class="form-control searchinput" style="width:auto" placeholder="Search">
            <div class="input-group-btn">
                <button class="btn btn-default" type="submit">
                    <i class="glyphicon glyphicon-search"></i>
                </button>
            </div>
        </div>
    </form>
</div>
<br />
<table id="Shippers" class="cell-border hover order-column">
    <thead>
        <tr>
            <th>
                Nume Companie
            </th>
            <th>
                Telefon
            </th>
            <th>
                Sterge
            </th>
        </tr>
    </thead>
<tbody></tbody>

</table>
